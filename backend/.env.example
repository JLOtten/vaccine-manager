# JWT Secret Key - Generate a secure random string for production
# You can generate one with: python -c "import secrets; print(secrets.token_urlsafe(32))"
COOKIE_SECRET_KEY="your-secret-key-change-in-production"

# JWT Algorithm (default: HS256)
JWT_ALGORITHM=HS256

# Access token expiration time in minutes (default: 1 day)
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS Origins - Comma-separated list of allowed origins (default: http://localhost:3000)
CORS_ORIGINS="http://localhost:3000"

SECURE_COOKIE=true
